algoritmo "ConteDe10A0"
var
   contador:Inteiro
inicio
      contador <- 10
      Enquanto (contador >= 0) faca
               EscrevaL(contador)
               contador <- contador - 1
               //contador <- contador - 2
               //contador <- contador - 3
      FimEnquanto
      EscrevaL("Terminei de contar")
fimalgoritmo
