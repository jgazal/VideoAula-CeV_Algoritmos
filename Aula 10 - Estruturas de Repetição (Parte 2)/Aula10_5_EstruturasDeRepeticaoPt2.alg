algoritmo "NumeroPrimo"
var
   C, N, ContDiv: Inteiro
inicio
      C <- 1
      ContDiv <- 0
      Escreva("Digite um número: ")
      Leia(N)
      Repita
         //Escreva(C)
         Se(N % C = 0) entao
            ContDiv <- ContDiv + 1
         FimSe
         C <- C + 1
      Ate(C > N)
      //EscrevaL(" Existem ",ContDiv," valores divisiveis")
      Se(ContDiv > 2) entao
         EscrevaL(" O número ", N," não é primo.")
      senao
         EscrevaL(" O número ", N," é primo.")
      FimSe
fimalgoritmo
